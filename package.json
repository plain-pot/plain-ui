{
  "name": "plain-ui",
  "version": "0.1.73",
  "description": "A components Library based on Vue.js",
  "main": "dist/plain-ui.umd.min.js",
  "files": [
    "dist",
    "src/styles"
  ],
  "keywords": [
    "vue",
    "vue.js",
    "component",
    "components",
    "ui",
    "framework",
    "plain"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build:page": "vue-cli-service build --analysis",
    "build": "vue-cli-service build --target lib --name plain-ui src/index.js --dest dist --analysis --prod",
    "start": "npm run serve"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "core-js": "^2.6.5",
    "element-resize-detector": "^1.1.15",
    "plain-utils": "^0.1.9",
    "popper.js": "^1.15.0",
    "scroll": "^3.0.1",
    "store": "^2.0.12",
    "tippy.js": "^4.3.1",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.6.0",
    "@vue/cli-service": "^3.6.0",
    "fecha": "^3.0.2",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.5.21"
  }
}
