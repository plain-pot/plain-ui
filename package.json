{
  "name": "plain-ui",
  "version": "0.1.81",
  "description": "A components Library based on Vue.js",
  "main": "dist/plain-ui.umd.min.js",
  "files": [
    "dist",
    "src/styles"
  ],
  "keywords": [
    "vue",
    "vue.js",
    "component",
    "components",
    "ui",
    "framework",
    "plain"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build:page": "cross-env env=prod vue-cli-service build --analysis",
    "build": "vue-cli-service build --target lib --name plain-ui src/index.js --dest dist --report --prod",
    "start": "npm run dev",
    "dev": "cross-env env=dev npm run serve",
    "prod": "cross-env env=prod npm run serve"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "core-js": "^2.6.5",
    "element-resize-detector": "^1.1.15",
    "popper.js": "^1.15.0",
    "scroll": "^3.0.1",
    "store": "^2.0.12",
    "tippy.js": "^4.3.1",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.6.0",
    "@vue/cli-service": "^3.6.0",
    "autoprefixer": "8.6.5",
    "cross-env": "5.2.0",
    "deepmerge": "4.2.2",
    "fecha": "3.0.2",
    "node-sass": "4.9.0",
    "plain-utils": "0.1.9",
    "postcss": "8.2.4",
    "sass-loader": "7.1.0",
    "vue-template-compiler": "2.5.21"
  }
}
